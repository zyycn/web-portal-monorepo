{
  "name": "web-portal-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "clean": "node ./scripts/clean/index.mjs",
    "dev": "turbo-run dev",
    "dev:all": "turbo run dev",
    "postinstall": "pnpm run -r --parallel stub",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "preview": "turbo-run preview",
    "reinstall": "pnpm cleandep && pnpm install",
    "updatedep": "pnpm update --latest -r"
  },
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.39.10"
  },
  "devDependencies": {
    "@internal/eslint-config": "workspace:^",
    "@internal/lint-staged-config": "workspace:^",
    "@internal/node-utils": "workspace:^",
    "@internal/postcss-config": "workspace:^",
    "@internal/prettier-config": "workspace:^",
    "@internal/stylelint-config": "workspace:^",
    "@internal/tailwindcss-config": "workspace:^",
    "@internal/tsconfig": "workspace:^",
    "@internal/vite-config": "workspace:^",
    "@packages/plugins": "workspace:^",
    "@packages/request": "workspace:^",
    "@packages/utils": "workspace:^",
    "@scripts/turbo-run": "workspace:^",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "stylelint": "^16.23.1",
    "tailwindcss": "^4.1.12",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": ">=22"
  },
  "volta": {
    "node": "22.18.0",
    "pnpm": "10.14.0"
  }
}
